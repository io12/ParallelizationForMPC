digraph G {
	
	node [style=rounded,shape=box]
	edge [dir=back]
	rankdir = BT;
  	ranksep = 0.5;
  	nodesep=0.4;
    // ranksep=0.5;

	subgraph s1 {
		b1 [label="A[1] = B[1];"];
		b2 [label="B[1] = A[1] * D[0];\nC[1] = A[1] * D[0];"];
		b3 [label="D[1] = B[1] * C[1];"];
		{rank = max; b1;}
		{rank = same; b2;}
		{rank = min; b3;}
		// {rank = same; b1; b2; b3;}
	}

	subgraph s2 {
		b4 [label="A[2] = B[2]"];
		b5 [label="B[2] = A[2] * D[1];\nC[2] = A[2] * D[1];"];
		b6 [label="D[2] = B[2] * C[2];"];
		{rank = max; b4;}
		{rank = same; b5;}
		{rank = min; b6;}
		// {rank = same; b4; b5; b6;}
	}

	b1 -> b2 -> b3 -> b5 -> b6;
	b4 -> b5;

	
	
	// {rank = same; t0;}
	// {rank = max; s1;}
	// {rank = min; s2;}
	
}
